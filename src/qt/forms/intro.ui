color:transparent;
selection-color:transparent;
}

QCheckBox {
spacing: 5px;
}

QCheckBox::indicator {
width: 16px;
height: 16px;
}

QCheckBox::indicator:unchecked {
image:url(':/images/unchecked');
}

QCheckBox::indicator:unchecked:hover {
image:url(':/images/unchecked');
}

